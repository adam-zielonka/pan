{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules" : {
    "no-empty": false,
    "semicolon": [true, "never"],
    "max-classes-per-file": false,
    "object-literal-sort-keys": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
